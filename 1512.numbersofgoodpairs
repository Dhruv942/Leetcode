/**
 * @param {number[]} nums
 * @return {number}
 */
var numIdenticalPairs = function(nums) {
    let size = nums.length;
    let pairs=0;
    for(let i = 0; i<size ;i++){
        for(let j = i+1; j<size ; j++){
            if(nums[i]==nums[j])
            pairs++;
        }
    }
    return pairs;
};
